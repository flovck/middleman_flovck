  <body>

  <div class="banner" style="background-image: linear-gradient(-225deg, rgba(0,101,168,0.6) 0%, rgba(0,36,61,0.6) 50%), url('images/thumb_IMG_0869_1024.jpg');">
    <div class="banner-content">
      <h1>Lancieux City Beach</h1>
      <p>Enjoy ;-)!</p>
      <%= link_to "GET IT", "#", class: "btn btn-primary btn-lg" %>
    </div>
  </div>

<div class="feature">
  <div class="container">
    <div class="row">
      <div class="col-xs-12 col-sm-6 col-md-3 text-center">
        <%= image_tag "globe.png" %>
        <h3>Discover the world</h3>
        <p>The earth is your new home.</p>
      </div>
      <div class="col-xs-12 col-sm-6 col-md-3 text-center">
        <%= image_tag "dollar.png" %>
        <h3>Pay online</h3>
        <p>No cash issues anymore.</p>
      </div>
      <div class="col-xs-12 col-sm-6 col-md-3 text-center">
        <%= image_tag "beer.png" %>
        <h3>Meet people</h3>
        <p>Discover new culture and lifestyle.</p>
      </div>
      <div class="col-xs-12 col-sm-6 col-md-3 text-center">
        <%= image_tag "heart.png" %>
        <h3>Share and love</h3>
        <p>Join a community you will love.</p>
      </div>
    </div>
  </div>
</div>



  <div class="wrapper-grey">
    <div class="container">
      <div class="row">
        <div class="col-xs-12 text-center">
        <h2>Your next trip</h2>
      </div>
      <div class="row">
        <div class="col-xs-12 col-sm-4">
          <div class="card card-paris">
            <div class="card-category">Paris</div>
              <div class="card-description">
              <h2>L'appart d'Anne</h2>
              <p>Very cool stuff, useful and smart</p>
              </div>
               <%= image_tag "anne.jpg", class: "card-user avatar" %>
              <%= link_to "", "#", class: "card-link" %>
            </div>
          </div>
      <div class="col-xs-12 col-sm-4">
          <div class="card" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.2)), url('images/card-brussels.jpeg');">
            <div class="card-category">Bruxelles</div>
              <div class="card-description">
                <h2>Le loft de Seb</h2>
                <p>Very cool stuff, useful and smart</p>
                </div>
                <%= image_tag "seb.jpg", class: "card-user avatar" %>
                <%= link_to "", "#", class: "card-link" %>
            </div>
          </div>
      <div class="col-xs-12 col-sm-4">
          <div class="card" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.2)), url('images/card-london.jpeg');">
            <div class="card-category">Popular</div>
              <div class="card-description">
                <h2>La chambre de Romain</h2>
                <p>Very cool stuff, useful and smart</p>
                </div>

                <%= image_tag "romain.jpeg", class: "card-user avatar" %>
                <%= link_to "", "#", class: "card-link" %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

  <div class="text-center">
    <%= link_to "see my team", "stylesheets/pages/team.html" %>
  </div>


  <div class="text-center padded">
  <h2>Get news from us</h2>
  <p>Leave us your mail and get news from us</p>
  <form class="form-inline">
  <input type="email" placeholder="bob@gmail.com" class="form-control">
  <input type="submit" value="Subscribe" class="btn btn-primary btn-bordered btn-bordered-primary">
  </form>
  </div>



  <div id="map" style="width: 100%; height: 400px;"></div>
  <div>


  </div>

      <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js"></script>

      <script type="text/javascript">
      var myLatlng = new google.maps.LatLng(48.852937,2.364178);

      var myOptions = {
      zoom: 16,
      center: myLatlng,
      scrollwheel: false,
      mapTypeId: google.maps.MapTypeId.ROADMAP,
      styles: []
      };

      var map = new google.maps.Map(document.getElementById('map'), myOptions);
      var marker = new google.maps.Marker({
        position: myLatlng,
        map: map,
        title: "You are here!"
      });
    </script>


  </div>
</body>


    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js"></script>
  </body>
</html>
